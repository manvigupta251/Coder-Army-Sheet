https://practice.geeksforgeeks.org/problems/check-if-two-arrays-are-equal-or-not3847/1?page=1&difficulty[]=-1&category[]=Arrays&sortBy=submissions

bool check(vector<ll> A, vector<ll> B, int N) {
        //code here

        sort(A.begin(), A.end());
        sort(B.begin(), B.end());
        
        for(int i=0; i<N; i++) {
            
                if(A[i] != B[i]) {
                    return false;
                }
            
        }
        
        return true;
    }
